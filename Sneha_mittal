class student{
    int roll;
    String add;
     int phone;
     
     student(int roll, long phone, String add){
        System.out.println("roll ");
        System.out.println(roll);
        System.out.println("phone ");
        System.out.println(phone);
        System.out.println("add ");
        System.out.println(this.add);
     }
     
}
public class assign {
    public static void main(String[] args) {
        
        System.out.println("name of student is:");
     System.out.println("sam");
     student obj1=new student(23, 123456789, "nkt");
     System.out.println("name of student is:");
     System.out.println("john");
     student obj2=new student(24, 1234567, "jaipur");
     

    }
}
ass2
class calculator{
    int a;
     int b;

     calculator(){}
        int fun1(int a, int b){
            return a+b;
        }
        double fun1(double a, int b){
            return a*b;
        }
     
     
}
public class testApp {
     public static void main(String[] args) {
        calculator cal = new calculator();
        System.out.println("Addition of two numbers is:");
        System.out.println(cal.fun1(2,3));
        System.out.println("Multipliaction  of two numbers is:");
        System.out.println(cal.fun1(2.33,4));
        
    }
}
ass3
class MyClass {
    int value;

   
   public MyClass() {
       value = 0;
   }

   // Parameterized constructor
   public MyClass(int val) {
       value = val;
   }

   // Method to display the value
   public void display() {
       System.out.println("Value: " + value);
   }

   // Overloaded method to display the sum of two values
   public void display(int num1, int num2) {
       System.out.println("Sum: " + (num1 + num2));
   }

   // Overloaded method to display the product of two values
   public void display(double num1, double num2) {
       System.out.println("Product: " + (num1 * num2));
   }
}

public class overloading_eg {
   public static void main(String[] args) {
       // Using constructors
       MyClass obj1 = new MyClass(); 
       MyClass obj2 = new MyClass(4); 

       // Using methods
       obj1.display(); 
       obj2.display(); 
       obj1.display(3, 2); 
       obj2.display(2.5, 7.9);
   }
}
ass4
class bird{
    String b1;
    String b2;
    bird(String b1){
        this.b1=b1;
        
    }
    void display(){
        System.out.println(b1);
        
    }
}
public class ass3 {
    public static void main(String[] args) {
        bird obj= new bird("pigeon");
        bird obj2=new bird("Parrot");
        obj.display();
        obj2.display();

    }
}
